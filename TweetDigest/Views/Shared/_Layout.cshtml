<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css", "~/Content/bootstrap")
</head>
<body>
    <header>
        <div class="content-wrapper">
            <div class="float-left">
                <p class="site-title">
                    <a href="~/">TweetDigest</a></p>
            </div>
        </div>
    </header>
    <div id="body">
        @RenderBody()
    </div>

    @RenderSection("scripts", required: false)
</body>
</html>
